{
  "title": "hobo",
  "name": "properjs-hobo",
  "description": "A very small, modular DOM utility for modern web apps.",
  "version": "0.2.2",
  "homepage": "https://github.com/ProperJS/hobo",
  "readmeFilename": "README.md",
  "contributors": [
    {
      "name": "Brandon Kitajchuk",
      "email": "kitajchuk@gmail.com",
      "url": "http://blkpdx.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ProperJS/hobo.git"
  },
  "bugs": {
    "url": "https://github.com/ProperJS/hobo/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ProperJS/hobo/blob/master/LICENSE"
    }
  ],
  "main": "hobo",
  "scripts": {
    "doc": "jsdoc lib/ -r -R README.md -P package.json -t node_modules/jsdoc-oblivion/template -c node_modules/jsdoc-oblivion/template/jsdoc.conf.json -d docs",
    "test": "npm run build -- 'append attr children closest detach eq filter index insertAfter insertBefore not parent prepend remove toggleClass trigger'",
    "posttest": "webpack test/test.js test/test-dist.js",
    "build": "node build.js",
    "watch": "chokidar ./test/test.js --initial -d 10 -c 'npm run test'"
  },
  "dependencies": {
    "properjs-matchelement": "^0.3.0"
  },
  "keywords": [
    "properjs",
    "event",
    "jquery",
    "data",
    "promise",
    "classes",
    "ajax",
    "xhr",
    "query",
    "dom"
  ],
  "devDependencies": {
    "jsdoc": "^3.4.0",
    "jsdoc-oblivion": "0.0.4",
    "chokidar-cli": "^1.0.1"
  }
}
