{
  "title": "hobo",
  "name": "properjs-hobo",
  "description": "All the things you want without all the cruft.",
  "version": "0.1.0",
  "homepage": "https://github.com/ProperJS/hobo",
  "readmeFilename": "README.md",
  "contributors": [
    {
      "name": "Brandon Kitajchuk",
      "email": "kitajchuk@gmail.com",
      "url": "http://blkpdx.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ProperJS/hobo.git"
  },
  "bugs": {
    "url": "https://github.com/ProperJS/hobo/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ProperJS/hobo/blob/master/LICENSE"
    }
  ],
  "main": "hobo",
  "scripts": {
    "watch": "chokidar 'hobo.js' 'lib/**/*.js' 'test/*.js' --initial -d 10 -c 'npm run tasks'",
    "tasks": "parallelshell 'npm run test' 'npm run build'",
    "posttasks": "uglifyjs --compress --mangle --output dist/hobo.min.js dist/hobo.js ; uglifyjs --compress --mangle --output dist/hobo-extended.min.js dist/hobo-extended.js",

    "test": "webpack test/test.js test/dist/test.js",
    "posttest": "webpack test/test-extended.js test/dist/test-extended.js",

    "build": "webpack hobo.js dist/hobo.js",
    "postbuild": "webpack hobo-extended.js dist/hobo-extended.js ; "
  },
  "dependencies": {
    "properjs-matchelement": "^0.3.0"
  },
  "keywords": [
    "properjs",
    "event",
    "jquery",
    "data",
    "promise",
    "classes",
    "ajax",
    "xhr",
    "query",
    "dom"
  ],
  "devDependencies": {
    "chokidar-cli": "^1.1.1",
    "parallelshell": "^2.0.0",
    "uglifyjs": "^2.4.10"
  }
}
